        <stentry>136</stentry>
      </strow>

      <strow>
        <stentry>Acronym</stentry>

        <stentry>Prescribing</stentry>
      </strow>

      <strow>
        <stentry>Name</stentry>

        <stentry>Prescribing</stentry>
      </strow>

      <strow>
        <stentry>Description</stentry>

        <stentry>Drug prescription data.</stentry>
      </strow>

      <strow>
        <stentry>Type</stentry>

